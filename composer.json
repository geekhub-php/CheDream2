{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.10.2",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.10.2.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "blueimp/jQuery-File-Upload",
                "version": "9.5.6",
                "dist": {
                    "url": "https://github.com/blueimp/jQuery-File-Upload/archive/9.5.6.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.4",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",
        "incenteev/composer-parameter-handler": "~2.0",

        "liip/imagine-bundle": "dev-master",
        "friendsofsymfony/user-bundle": "2.0.*@dev",
        "doctrine/doctrine-fixtures-bundle": "2.2.*@dev",
        "stof/doctrine-extensions-bundle": "1.1.*@dev",
        "hwi/oauth-bundle": "0.3.*@dev",
        "twbs/bootstrap": "v3.0.0",
        "frameworks/jquery": "dev-master",
        "jquery/jquery":  "1.10.*",
        "blueimp/jQuery-File-Upload": "9.5.6",
        "nervo/yuicompressor": "dev-master",
        "fpn/doctrine-extensions-taggable": "dev-master",
        "fpn/tag-bundle":"dev-master",
        "sonata-project/admin-bundle": "2.2.*@dev",
        "sonata-project/doctrine-orm-admin-bundle": "2.2.*@dev",
        "sonata-project/media-bundle": "2.2.*@dev",
        "ahonymous/fontello": "dev-master",
        "braincrafted/bootstrap-bundle": "~2.0@alpha",
        "jms/serializer-bundle": "0.13.*@dev",
        "h4cc/alice-fixtures-bundle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "mkdir -p web/fonts && cp -u vendor/twbs/bootstrap/dist/fonts/* web/fonts/",
            "mkdir -p web/fonts && cp -u vendor/ahonymous/fontello/fonts/* web/fonts/"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "mkdir -p web/fonts && cp -u vendor/twbs/bootstrap/dist/fonts/* web/fonts/",
            "mkdir -p web/fonts && cp -u vendor/ahonymous/fontello/fonts/* web/fonts/"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
