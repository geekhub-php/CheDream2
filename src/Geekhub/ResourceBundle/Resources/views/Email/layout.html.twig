{% extends '::base.html.twig' %}

{% block stylesheets %}
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet">{% endblock %}

{% block body %}
    <div id="wrapper" class="container">
        <header class="row">
            <div class="col-md-6 col-md-offset-3">
                {% block header %}
                    <h1>Header</h1>
                {% endblock header %}
            </div>
        </header>

        <main class="row">
            {% block content %}
                <p>Content</p>
            {% endblock content %}
        </main>
    </div>
{% endblock body %}

{% block javascript %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
{% endblock javascript %}

